{
   "name": "myzap",
   "version": "1.0.0",
   "description": "Free OpenSource Whatsapp Api",
   "main": "server.ts",
   "scripts": {
      "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/server.ts",
      "start": "ts-node src/index.ts",
      "test": "jest",
      "migration:create": "typeorm migration:create -d src/database/migration -n ",
      "migration:run": "typeorm migration:run",
      "migration:revert": "typeorm migration:revert",
      "typeorm": "ts-node-dev  ./node_modules/typeorm/cli.js",
      "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
      "swagger": "node dist/swagger.js"
   },
   "author": "Bill Barsch",
   "license": "ISC",
   "dependencies": {
      "axios": "^0.21.1",
      "bcryptjs": "^2.4.3",
      "bluebird": "^3.7.2",
      "body-parser": "^1.19.0",
      "class-validator": "^0.13.1",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-async-errors": "^3.1.1",
      "jsonwebtoken": "^8.5.1",
      "mime": "^2.4.6",
      "multer": "^1.4.2",
      "mysql": "^2.14.1",
      "mysql2": "^2.2.5",
      "qrcode-terminal": "^0.12.0",
      "reflect-metadata": "^0.1.10",
      "sequelize": "^6.5.0",
      "sequelize-typescript": "^2.1.0",
      "swagger-autogen": "^1.2.5",
      "swagger-jsdoc": "^6.0.6",
      "swagger-ui-express": "^4.1.6",
      "ts-node-dev": "^1.0.0",
      "typeorm": "0.2.31",
      "typescript": "^4.0.3",
      "typescript-rest-swagger": "^1.1.5",
      "uuidv4": "^6.1.0",
      "venom-bot": "^2.2.4",
      "whatsapp-web.js": "^1.12.4"
   },
   "devDependencies": {
      "@babel/cli": "^7.10.3",
      "@babel/core": "^7.10.3",
      "@babel/node": "^7.10.3",
      "@babel/plugin-proposal-class-properties": "^7.10.1",
      "@babel/plugin-proposal-decorators": "^7.10.3",
      "@babel/plugin-transform-flow-strip-types": "^7.10.1",
      "@babel/preset-env": "^7.10.3",
      "@babel/preset-typescript": "^7.10.1",
      "@types/cors": "^2.8.8",
      "@types/express": "^4.17.8",
      "@types/jest": "^26.0.15",
      "@types/multer": "^1.4.4",
      "@types/node": "^8.0.29",
      "@types/sequelize": "^4.28.9",
      "@types/swagger-jsdoc": "^3.0.2",
      "@types/swagger-ui-express": "^4.1.2",
      "@typescript-eslint/eslint-plugin": "^4.5.0",
      "@typescript-eslint/parser": "^4.5.0",
      "babel-plugin-module-resolver": "^4.0.0",
      "cross-env": "^7.0.3",
      "eslint": "6.8.0",
      "eslint-config-airbnb-base": "^14.2.0",
      "eslint-config-prettier": "^6.14.0",
      "eslint-import-resolver-typescript": "^2.3.0",
      "eslint-plugin-import": "2.21.2",
      "eslint-plugin-import-helpers": "^1.1.0",
      "eslint-plugin-prettier": "^3.1.4",
      "prettier": "^2.1.2",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.9.0"
   }
}
